package Projekti;

import javafx.event.ActionEvent;
import javafx.event.EventHandler;

class SignInEvent implements EventHandler<ActionEvent>
{
	FormValidation validate = new FormValidation();
	public void handle(ActionEvent e)
	{
		validate.ntextFieldNotEmpty(SignInForm.FirstNameTextField, SignInForm.FirstName, "Shëno emrin!");
		validate.ltextFieldNotEmpty(SignInForm.LastNameTextField, SignInForm.LastName, "Shëno mbiemrin!");
	}
}
